print ("Введите имя:")
a = input()
print ("Введите фамилию:")
b = input()
print ("Введите номер группы:")
c = input()
print(a, b, "(", c, ")" )






year = int(input('Введите год:'))
if (year % 4 == 0 and year % 100 !=0) or year % 400 == 0:
    print ('Год високосный.')
else:
    print ('Год не високосный.')
